diff --git a/contentview.swift b/contentview.swift
new file mode 100644
index 0000000..c713f4f
--- /dev/null
+++ b/contentview.swift
@@ -0,0 +1,98 @@
+import SwiftUI
+
+struct ContentView: View {
+    @State private var selectedTab = 0
+    @AppStorage("isDarkMode") private var isDarkMode: Bool = true
+    @EnvironmentObject var lang: LanguageManager
+    
+    var body: some View {
+        TabView(selection: $selectedTab) {
+            HomeView
+                .tabItem { Label(localized("Home"), systemImage: "house.fill") }
+                .tag(0)
+            
+            ReadyView()
+                .tabItem { Label(localized("Ready"), systemImage: "building.2.fill") }
+                .tag(1)
+            
+            OffPlanView()
+                .tabItem { Label(localized("Off-Plan"), systemImage: "cube.fill") }
+                .tag(2)
+            
+            RentingView()
+                .tabItem { Label(localized("Rent"), systemImage: "key.fill") }
+                .tag(3)
+            
+            AboutView()
+                .tabItem { Label(localized("About"), systemImage: "info.circle.fill") }
+                .tag(4)
+        }
+        .preferredColorScheme(isDarkMode ? .dark : .light)
+        .environment(\.layoutDirection, lang.isArabic ? .rightToLeft : .leftToRight)
+        .overlay(alignment: .topTrailing) {
+            HStack {
+                Button {
+                    lang.toggleLanguage()
+                } label: {
+                    Image(systemName: "globe")
+                        .font(.title3)
+                        .padding(8)
+                        .background(.ultraThinMaterial, in: Circle())
+                }
+                
+                Button {
+                    isDarkMode.toggle()
+                } label: {
+                    Image(systemName: isDarkMode ? "sun.max.fill" : "moon.fill")
+                        .font(.title3)
+                        .padding(8)
+                        .background(.ultraThinMaterial, in: Circle())
+                }
+            }
+            .padding()
+        }
+    }
+}
+
+// simple localization helper
+func localized(_ key: String) -> String {
+    let ar: [String:String] = [
+        "Home":"Ø§Ù„Ø±Ø¦ÙŠØ³ÙŠØ©","Ready":"Ø¬Ø§Ù‡Ø²","Off-Plan":"Ø®Ø·Ø© Ù…Ø³Ø¨Ù‚Ø©","Rent":"Ø¥ÙŠØ¬Ø§Ø±",
