diff --git a/LanguageManager.swift b/LanguageManager.swift
new file mode 100644
index 0000000..20294b0
--- /dev/null
+++ b/LanguageManager.swift
@@ -0,0 +1,25 @@
+//
+//  LanguageManager.swift
+//  Xsite Broker
+//
+//  Created by Faraz on 19/10/2025.
+//
+
+import Foundation
+import Combine
+import SwiftUI
+
+@MainActor
+final class LanguageManager: ObservableObject {
+    @Published var isArabic: Bool = false
+    
+    var currentLocale: Locale {
+        Locale(identifier: isArabic ? "ar" : "en")
+    }
+    
+    func toggleLanguage() {
+        withAnimation(.spring()) {
+            isArabic.toggle()
+        }
+    }
+}
