+++ b/Assets./AppState.swift
@@ -0,0 +1,46 @@
+//import SwiftUI
+
+struct AppTheme: Equatable, Sendable {
+    // Follow system by default. Set to .light or .dark to force.
+    var preferredColorScheme: ColorScheme? = nil
+
+    // Accent color for your app. Defaults to the appâ€™s accent color.
+    var accentColor: Color = .accentColor
+
+    mutating func toggleColorScheme() {
+        switch preferredColorScheme {
+        case .some(.light): preferredColorScheme = .dark
+        case .some(.dark): preferredColorScheme = nil // back to system
+        case nil: preferredColorScheme = .light
+        @unknown default: preferredColorScheme = nil
+        }
+    }
+}
+//  AppState.swift
+//  Xsite Broker
+//
+//  Created by Faraz on 19/10/2025.
+//
+
+import SwiftUI
+import Observation
+
+@Observable
+final class AppState {
+    var theme = AppTheme()
+    var lang = L10n()
+}
+import Foundation
+
+public struct L10n: Sendable, Equatable {
+    public init() {}
+
+    // Convenience for localized strings if you need it.
+    public func string(_ key: String,
+                       table: String? = nil,
+                       bundle: Bundle = .main,
+                       value: String = "",
+                       comment: String = "") -> String {
+        NSLocalizedString(key, tableName: table, bundle: bundle, value: value, comment: comment)
+    }
+}
