+import SwiftUI
+
+struct ContentView: View {
+    @EnvironmentObject var lang: LanguageManager
+    @AppStorage("isDarkMode") private var isDarkMode = true
+    @State private var tab = 0
+    
+    var body: some View {
+        TabView(selection: $tab) {
+            HomeView().tabItem { Label(localized("Home"), systemImage: "house.fill") }.tag(0)
+            ReadyView().tabItem { Label(localized("Ready"), systemImage: "building.2.fill") }.tag(1)
+            OffPlanView().tabItem { Label(localized("Off-Plan"), systemImage: "cube.fill") }.tag(2)
+            RentingView().tabItem { Label(localized("Rent"), systemImage: "key.fill") }.tag(3)
+            AboutView().tabItem { Label(localized("About"), systemImage: "info.circle.fill") }.tag(4)
+        }
+        .preferredColorScheme(isDarkMode ? .dark : .light)
+        .environment(\.layoutDirection, lang.isArabic ? .rightToLeft : .leftToRight)
+        .overlay(alignment: .topTrailing) {
+            HStack {
+                Button { lang.toggle() } label: {
+                    Image(systemName: "globe")
+                        .padding(8).background(.ultraThinMaterial, in: Circle())
+                }
+                Button { isDarkMode.toggle() } label: {
+                    Image(systemName: isDarkMode ? "sun.max.fill" : "moon.fill")
+                        .padding(8).background(.ultraThinMaterial, in: Circle())
+                }
+            }.padding()
+        }
+    }
+}
