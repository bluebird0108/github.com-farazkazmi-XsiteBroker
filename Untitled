+//
+//  LanguageManager.swift
+//  Xsite Broker
+//
+//  Created by Faraz on 19/10/2025.
+//
+
+
+import SwiftUI
+
+final class LanguageManager: ObservableObject {
+    @Published var isArabic = false
+    var currentLocale: Locale { Locale(identifier: isArabic ? "ar" : "en") }
+    
+    func toggle() { withAnimation(.easeInOut) { isArabic.toggle() } }
+}
+
+func localized(_ key: String) -> String {
+    let ar: [String: String] = [
+        "Home": "Ø§Ù„Ø±Ø¦ÙŠØ³ÙŠØ©", "Ready": "Ø¬Ø§Ù‡Ø²", "Off-Plan": "Ø®Ø·Ø© Ù…Ø³Ø¨Ù‚Ø©",
+        "Rent": "Ø¥ÙŠØ¬Ø§Ø±", "About": "Ø¹Ù† Ø§Ù„Ø´Ø±ÙƒØ©",
+        "Meet Our Team": "ÙØ±ÙŠÙ‚Ù†Ø§", "Register Interest": "Ø³Ø¬Ù„ Ø§Ù‡ØªÙ…Ø§Ù…Ùƒ",
+        "Mortgage Calculator": "Ø­Ø§Ø³Ø¨Ø© Ø§Ù„Ø±Ù‡Ù†", "RERA Index": "Ù…Ø¤Ø´Ø± Ø±ÙŠÙ”Ù€Ø±Ø§",
+        "Contact Us": "ØªÙˆØ§ØµÙ„ Ù…Ø¹Ù†Ø§",
+        "Ready Properties": "Ø§Ù„Ø¹Ù‚Ø§Ø±Ø§Øª Ø§Ù„Ø¬Ø§Ù‡Ø²Ø©",
+        "Off-Plan Projects": "Ù…Ø´Ø§Ø±ÙŠØ¹ Ø¹Ù„Ù‰ Ø§Ù„Ù…Ø®Ø·Ø·",
+        "Renting Options": "Ø®ÙŠØ§Ø±Ø§Øª Ø§Ù„Ø¥ÙŠØ¬Ø§Ø±"
+    ]
+    return ar[key] ?? key
+}
