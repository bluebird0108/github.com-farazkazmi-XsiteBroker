+import Foundation
+import SwiftUI
+
+struct HomeView: View {
+    var body: some View {
+        ScrollView {
+            VStack(spacing: 24) {
+                Image("logo_Xsite")
+                    .resizable()
+                    .scaledToFit()
+                    .frame(height: 120)
+                    .padding(.top, 40)
+                
+                Text("Xsite Real Estate Brokers LLC")
+                    .font(.title2.bold())
+                    .multilineTextAlignment(.center)
+                
+                Text("Your trusted Dubai real estate experts â€” Ready, Off-Plan, and Leasing Services")
+                    .font(.body)
+                    .multilineTextAlignment(.center)
+                    .padding(.horizontal)
+                
+                Divider()
+                
